ul {
  display: block;
  
  li {
    display: inline;
    &.error {
      color: red;
    }
  }
}

ul li {
  color: #000;
  background-color: gray;
  
  &.info {
    color: yellow;
  }
}

ul li.details { color: green; }

ul {
  color: #123;
  
  li {
    font-weight: normal;
  }
}
